{
  "name": "codemixer",
  "version": "1.0.0",
  "description": "Code mixer created in JavaScript, Node.js and Electron.js",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "jest",
    "run": "electron-forge make && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoxton314/CodeMixer.git"
  },
  "keywords": [
    "code",
    "mixer",
    "shuffle",
    "randomizer"
  ],
  "author": "hoxton314",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hoxton314/CodeMixer/issues"
  },
  "homepage": "https://github.com/hoxton314/CodeMixer#readme",
  "dependencies": {
    "electron": "^12.0.2",
    "voca": "^1.4.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "sokobanbanban"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
